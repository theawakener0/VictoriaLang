// Simple test of new features
print("=== Testing Const ===")
const x = 5
print("const x = 5, x is: ")
print(x)

print("\n=== Testing Slice ===")
let arr = [1, 2, 3, 4, 5]
print("arr = [1, 2, 3, 4, 5]")

let s1 = arr[1:3]
print("arr[1:3] = ")
print(s1)

let s2 = arr[:2]
print("arr[:2] = ")
print(s2)

let s3 = arr[3:]
print("arr[3:] = ")
print(s3)

print("\n=== Testing Spread ===")
let a = [1, 2, 3]
let b = [4, 5, 6]
print("a = [1, 2, 3]")
print("b = [4, 5, 6]")

let merged = [...a, ...b]
print("[...a, ...b] = ")
print(merged)

let extended = [0, ...a, 99]
print("[0, ...a, 99] = ")
print(extended)

print("\n=== All tests passed! ===")
